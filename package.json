{
	"name": "celeri-ui",
	"license": "MIT",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"build": "tsc && vite build",
		"commit": "cz",
		"dev": "vite",
		"prepare": "husky install",
		"serve": "vite preview",
		"test": "cross-env TS_NODE_PROJECT=\"mocha/tsconfig.json\" mocha --exit -r ts-node/register -r global-jsdom/register 'mocha/tests/**/*.{ts,tsx}'",
		"test:ci": "npm run test",
		"format": "prettier -uw --ignore-path .gitignore .",
		"run-tsc": "tsc",
		"run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx src",
		"run-stylelint": "stylelint --cache --fix --ignore-path .gitignore '**/*.css'",
		"lint": "run-p run-tsc run-eslint run-stylelint",
		"validate": "run-p lint test:ci test:e2e:headless",
		"lint-staged": "lint-staged",
		"docs:dev": "cd ./docs && mdbook serve -p 3001",
		"docs:build": "cd ./docs && mdbook build"
	},
	"dependencies": {
		"@mapbox/mapbox-gl-draw": "^1.3.0",
		"@reduxjs/toolkit": "^1.8.1",
		"@turf/turf": "^6.5.0",
		"mapbox-gl": "^1.13.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-query": "3.34.2",
		"react-redux": "^7.2.8",
		"react-router-dom": "5.3.0",
		"redux-undo": "^1.0.1"
	},
	"devDependencies": {
		"@babel/core": "7.16.0",
		"@babel/preset-env": "7.16.4",
		"@babel/preset-react": "7.16.0",
		"@babel/preset-typescript": "7.16.0",
		"@nabla/vite-plugin-eslint": "1.3.5",
		"@tailwindcss/forms": "0.4.0",
		"@testing-library/dom": "8.11.1",
		"@testing-library/react": "12.1.2",
		"@testing-library/react-hooks": "7.0.2",
		"@testing-library/user-event": "13.5.0",
		"@types/chai": "^4.3.1",
		"@types/css-mediaquery": "0.1.1",
		"@types/jsdom": "^16.2.14",
		"@types/mapbox-gl": "^2.7.0",
		"@types/mapbox__mapbox-gl-draw": "^1.2.3",
		"@types/mocha": "^9.1.0",
		"@types/react": "17.0.37",
		"@types/react-dom": "17.0.11",
		"@types/react-redux": "^7.1.24",
		"@types/react-router-dom": "5.3.2",
		"@types/sinon": "^10.0.11",
		"@types/wicg-file-system-access": "^2020.9.5",
		"@typescript-eslint/eslint-plugin": "5.6.0",
		"@typescript-eslint/parser": "5.6.0",
		"@vitejs/plugin-react": "1.1.2",
		"autoprefixer": "10.4.0",
		"chai": "^4.3.6",
		"concurrently": "^6.5.1",
		"cross-env": "^7.0.3",
		"css-mediaquery": "0.1.2",
		"eslint": "8.4.1",
		"eslint-config-airbnb": "19.0.2",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "16.1.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-import-resolver-node": "^0.3.6",
		"eslint-plugin-import": "2.25.3",
		"eslint-plugin-jest": "25.3.0",
		"eslint-plugin-jest-dom": "5.5.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-react": "7.27.1",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-react-prefer-function-component": "0.0.7",
		"eslint-plugin-testing-library": "5.0.1",
		"eslint-plugin-unicorn": "39.0.0",
		"global-jsdom": "^8.4.0",
		"husky": "7.0.4",
		"jsdom": "^19.0.0",
		"lint-staged": "12.1.2",
		"mocha": "^9.2.2",
		"msw": "0.36.3",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.5",
		"prettier": "2.5.1",
		"rollup": "2.61.1",
		"rollup-plugin-istanbul": "3.0.0",
		"sinon": "^12.0.1",
		"start-server-and-test": "1.14.0",
		"stylelint": "14.1.0",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-config-standard": "24.0.0",
		"tailwindcss": "3.0.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"vite": "2.7.2",
		"vite-plugin-pwa": "0.11.11",
		"vite-tsconfig-paths": "3.3.17",
		"whatwg-fetch": "3.6.2",
		"workbox-build": "6.4.2",
		"workbox-window": "6.4.2"
	},
	"browserslist": {
		"production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*": "prettier -uw",
		"*.css": "stylelint --cache --fix",
		"*.{ts,tsx}": [
			"eslint --cache --fix"
		]
	}
}
